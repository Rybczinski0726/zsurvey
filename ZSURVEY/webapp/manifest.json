{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.sds.erp.ZSURVEY",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.sds.erp.ZSURVEY.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sds.erp.ZSURVEY.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.sds.erp.ZSURVEY.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": [
						"startpage"
					]
				}
			},
			"routes": [
				{
					"name": "RouteApp",
					"pattern": "",
					"target": [
						"startpage"
					]
				},
				{
					"name": "personal",
					"pattern": "personal",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"personal"
					]
				},
				{
					"name": "PersonalInfo",
					"pattern": "personalInfo",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"PersonalInfo"
					]
				},
				{
					"name": "Survey",
					"pattern": "survey",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Survey"
					]
				},
				{
					"name": "Submit",
					"pattern": "submit",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Submit"
					]
				}
			],
			"targets": {
				"startpage": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Startpage"
				},
				"personal": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Personal",
					"viewLevel": 1
				},
				"PersonalInfo": {
					"viewType": "XML",
					"viewName": "PersonalInfo",
					"viewLevel": 1
				},
				"Survey": {
					"viewType": "XML",
					"viewName": "Survey",
					"viewLevel": 1
				},
				"Submit": {
					"viewType": "XML",
					"viewName": "Submit",
					"viewLevel": 1
				}
			}
		}
	}
}